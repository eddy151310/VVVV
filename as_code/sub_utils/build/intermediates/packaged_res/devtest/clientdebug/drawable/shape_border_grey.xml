<?xml version="1.0" encoding="utf-8"?><!--定义一个带圆角,白色背景,绿色边框的矩形-->
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!--填充颜色-->
    <solid android:color="@color/CC_FFFFFF" />
    <!--描边-->
    <stroke
        android:width="@dimen/dp_1"
        android:color="@color/CC_E6E6E9" />
    <corners
        android:radius="@dimen/dp_4" />
</shape>