<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/CC_F1F2F6"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/CC_FFFFFF">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_marginLeft="@dimen/dp_15"
            android:layout_marginRight="@dimen/dp_15"
            android:layout_marginTop="@dimen/dp_10"
            android:gravity="center"
            android:text="@string/Setting_I0"
            android:textColor="@color/CC_919399"
            android:textSize="@dimen/text_size_12" />

    </LinearLayout>


    <View style="@style/line_style" />

    <RelativeLayout
        android:id="@+id/re_dialog_logout"
        style="@style/common_item_style">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/Setting_G0"
            android:textColor="@color/CC_D63031"
            android:textSize="@dimen/text_size_17" />
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/re_dialog_cancel"
        style="@style/common_item_style"
        android:layout_marginTop="@dimen/dp_5">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="@string/Setting_H0"
            android:textColor="@color/CC_282934"
            android:textSize="@dimen/text_size_17" />
    </RelativeLayout>

</LinearLayout>